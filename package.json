{
	"name": "data",
	"version": "1.0.0",
	"description": "Data processing",
	"main": "build/index.js",
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"prebuild": "rm -rf build",
		"start-ts": "ts-node ./src/index.ts",
		"before-commit": "run-s format",
		"clean": "rm -rf node_modules",
		"format": "pretty-quick --staged",
		"start": "node ./build/index.js",
		"test": "echo 'no tests' && exit 0",
		"dev": "run-p watch-*",
		"watch-start": "nodemon --watch build --delay 500ms -e js,json,html,md -x 'npm start'",
		"watch-test": "nodemon --watch src -e ts,json,html,md -x 'npm test'",
		"watch-ts": "tsc-watch --noClear -p tsconfig.build.json"
	},
	"keywords": [
		"data",
		"parsing",
		"processing"
	],
	"license": "ISC",
	"author": {
		"email": "npm@iampeterbanjo.com",
		"name": "Peter Banjo"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run before-commit"
		}
	},
	"dependencies": {
		"@hapi/boom": "^8.0.1",
		"@hapi/hapi": "^18.4.0",
		"@hapi/joi": "^15.1.1",
		"agenda": "^2.3.0",
		"csvtojson": "^2.0.10",
		"env": "git+https://github.com/iampeterbanjo/iampeterbanjo.com-env.git",
		"hapi-hodor": "git+https://github.com/iampeterbanjo/hapi-hodor.git",
		"models": "git+https://github.com/iampeterbanjo/iampeterbanjo.com-models.git",
		"ramda": "^0.26.1",
		"rxjs": "^6.5.3",
		"time": "file:../time",
		"typescript": "^3.7.3",
		"xmlhttprequest": "^1.8.0"
	},
	"devDependencies": {
		"@hapi/code": "^6.0.0",
		"@hapi/lab": "^20.4.0",
		"@types/pino": "^5.14.0",
		"@types/ramda": "^0.26.38",
		"husky": "^3.1.0",
		"nodemon": "^1.19.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.19.1",
		"pretty-quick": "^2.0.1",
		"ts-node": "^8.5.4",
		"tsc-watch": "^4.0.0"
	}
}
